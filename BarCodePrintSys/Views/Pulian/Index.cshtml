        function Check() {
            var cnbqywm = document.getElementById("cnbqywm").value;
            try {
                $.ajax({
                    type: "GET",
                    url: '../Datainself/Getdata',
                    data: { scph: cnbqywm },
                    dataType: "json",
                    async: false,  //设置异步，必须先更改对应值才能做后续的传递参数操作
                    success: function (data) {
                        document.getElementById("pllh").value = data.Tables[0].Rows[0].customer_goods_id;
                        document.getElementById("gysdm").value = '10332';
                        document.getElementById("aslh").value = data.Tables[0].Rows[0].company_materials_id;
                        document.getElementById("ggxh").value = data.Tables[0].Rows[0].goods_name + ' ' + data.Tables[0].Rows[0].size_id;
                        document.getElementById("scph").value = data.Tables[0].Rows[0].number;
                        document.getElementById("sl").value = data.Tables[0].Rows[0].qty;
                        document.getElementById("sczq").value = data.Tables[0].Rows[0].dc;
                        //for (var i = 0; i < data.Tables[0].Rows.length; i++) {
                        //    $("#wlbh").append("<option value=" + data.Tables[0].Rows[i].s_wlbh + ">" + data.Tables[0].Rows[i].s_wlbh + "</option>")
                        //    $("#n_wlbh").append("<option value=" + data.Tables[0].Rows[i].s_wlbh + ">" + data.Tables[0].Rows[i].s_wlbh + "</option>")
                        //}
                    },
                    error: function (res) {
                        layer.msg('查询出错或者无数据！', { icon: 0 });
                    }
                });
            } catch (e) {
                console.log(e);
            }
        }
