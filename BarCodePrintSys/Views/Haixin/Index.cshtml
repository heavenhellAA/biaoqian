                var x = document.getElementById("textarea").getElementsByTagName("input");
                //填写信息是否完整
                for (var i = 0; i < x.length; i++) {
                    if (x[i].value == "") {
                        alert('信息未填写完整');
                        x[i].focus();
                        return false;// 有空值
                    }
                }
                var gysdm = document.getElementById("gysdm").value;//供应商代码
                var haixinType = document.getElementById("haixinType").value;//供应商类别
                var khlh = document.getElementById("khlh").value;//Code Number：客户料号
                var wlms = document.getElementById("wlms").value;//Description：物料描述
