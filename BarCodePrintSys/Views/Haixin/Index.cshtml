                    dataType: "text",
                    success: function (data) {
                        var str = data.substring(7);
                        var scrq_32 = data.substring(2, 6);
                        var arr = str.split(',');
                        if (data.substring(0, 1) > 0) {
                            var num1 = $("#numcount").val();
                            var headstr = "<html><body>";
                            var footstr = "</body></html>";
                            var printData1 = '';
                            var fenye = '<div style="page-break-after:always;"></div>';    //分页


                            for (i = 0 ; i < num1 ; i++) {
                                var onecodemsg = gysdm + '-' + khlh + '-' + scrq_32 + '-' + arr[i] +sldmsg;
                                $("#onecode-wl").barcode(onecodemsg, 'code128', { barWidth: 1, barHeight: 32, fontSize: 6, showHRI: false });
                                $("#onecode-wl_msg").html(onecodemsg);
                                printData1 += document.getElementById("PrintArea").innerHTML;
                                if (i != num1 - 1) {
                                    printData1 = printData1 + fenye;
                                }
