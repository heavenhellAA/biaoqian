<body>
    <div id="layerDemo" style="margin-bottom: 0;">
        <div class="layui-btn-container" style="display: none;">
            <button data-method="notice" class="layui-btn">弹出标签使用方法以及更新说明</button>
        </div>
    </div>

    <!--海信电子-->
    <div style="width: 100%; height: 430px; display: none" id="pint_wai">
        <div id="textarea" style="width: 400px; height: 360px; margin-top: 20px; line-height: 25px; margin-left: 50px; text-align: right; display: inline-block;">
            <div style="width: 400px;" class="font">
                厂内标签一维码:
				<input id="cnbqywm" type="text" onblur="Check();" />
            </div>
            <div class="font" style="width: 400px;">
                客户类型:
				<select id="haixinType" style="width: 203px;" disabled>
                    <option value="0">海信电子</option>
                    <option value="1">海信空调</option>
                </select>
            </div>
            <div class="font" style="width: 400px;">
                供应商代码:
				<select id="gysdm">
                    <option value="35142">35142海信(广东、贵阳、青岛海信电器)电子</option>
                    <option value="202125">202125海信(山东、广东、浙江)空调</option>
                    <option value="101549">101549青岛智动精工电子（广东海信通信）</option>
                </select>
            </div>
            <div style="width: 400px;" class="font">
                客户料号:
				<input id="khlh" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                物料描述:
				<input id="wlms" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                数量:
				<input id="sl" />
            </div>
            <div style="width: 400px;" class="font">
                生产日期:
				<input id="scrq" />
            </div>
            <div style="width: 400px;" class="font">
                QC:
				<input id="qc" value="QC18" readonly />
            </div>
            <div style="width: 400px;" class="font">
                生产批号:
				<input id="scph" />
            </div>
            <div style="width: 400px;" class="font">
                备注:
				<input id="bz" style="width: 240px;" />
            </div>
            <div style="width: 400px;" class="font">
                打印张数:
				<input type="number" id="numcount" oninput="if(value > 999 || value < 1 ){alert('打印张数范围在1到999之间！');value = ''}" />
            </div>
            <div style="width: 400px;" class="font">
                是否为遗漏补打:
					<label>
                        <input type="checkbox" name="ylbd" value="1">是</label>
            </div>
            <div style="width: 400px;" class="font">
                是否为试流单:
					<label>
                        <input type="checkbox" name="sld" value="1">是</label>
            </div>
            <div style="width: 400px; height: 45px; text-align: right;">
                @* <button id="Eliminate" class="bot" style="border: 5px solid #ffd68c; display: inline-block; background-color: orange; color: white;">清空</button>*@
                <button id="Printing" class="bot" style="border: 5px solid #ffd68c; display: inline-block; background-color: seagreen; color: white;">打印</button>
            </div>
        </div>




        <!--右侧打印区域-->
        <div id="PrintArea" style="width: 100mm; height: 100mm; vertical-align: top; display: inline-block; margin-left: 20px; white-space: nowrap; font-family: SimSun,monospace;">
            <!--使用表格显示-->
            <table style="border: thin; width: 96mm; height: 98mm; margin-left: 4px; margin-top: 4px;" rules="rows">
                <tbody>
                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1" style="border-right-style: hidden;"></td>
                        <td colspan="2" class="txt2">HuNan Aihua Group CO.,LTD</td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Code Number:</td>
                        <td colspan="2" class="txt2" id="khlh_msg"></td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Description:</td>
                        <td colspan="2" class="txt2" id="wlms_msg"></td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Quantity:</td>
                        <td colspan="2" class="txt2" id="sl_msg"></td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Produce date:</td>
                        <td colspan="2" class="txt2" id="scrq_msg"></td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Check:</td>
                        <td colspan="2" class="txt2">QC18</td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Quality:</td>
                        <td colspan="2" class="txt2">Pass</td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Batch:</td>
                        <td colspan="2" class="txt2" id="scph_msg"></td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="1" class="txt1">Remark:</td>
                        <td colspan="2" class="txt2" id="bz_msg"></td>
                    </tr>

                    <tr style="height: 19px;">
                        <td colspan="3" class="txt3" style="text-align: right; padding-right: 10px;">Exemption from Inspection </td>
                    </tr>

                    <tr style="height: 38px;">
                        @*                        <td colspan="1" class="txt1" style="border-right-style: hidden;"></td>*@
                        <td colspan="3" id="onecode-wl" style="border: 1px solid #000000; border-right-style: solid; border-left-style: solid; border-bottom-style: hidden;"></td>
                    </tr>

                    <tr style="height: 19px;">
                        @*<td colspan="1" class="txt1" style="border-right-style:hidden;"></td>*@
                        <td colspan="3" id="onecode-wl_msg" style="border: 1px solid #000000; border-right-style: solid; border-left-style: solid; padding-left: 10px; border-bottom-style: hidden;"></td>
                    </tr>

                    <tr style="height: 38px;">
                        @* <td colspan="1" class="txt1" style="border-right-style:hidden;"></td>*@
                        <td colspan="3" id="onecode-sl" style="border: 1px solid #000000; border-right-style: solid; border-left-style: solid; border-bottom-style: hidden;"></td>
                    </tr>

                    <tr style="height: 19px;">
                        @*<td colspan="1" class="txt1" style="border-right-style:hidden;"></td>*@
                        <td colspan="3" id="onecode-sl_msg" style="border: 1px solid #000000; border-right-style: solid; border-left-style: solid; padding-left: 10px;"></td>
                    </tr>

                    <tr style="height: 66px;">
                        <td colspan="1" class="txt1" id="scy_msg" style="font-size: 28px; font-weight: 800; text-align: center"></td>
                        <td colspan="2" class="txt2">
                            <img src="../../Images/img/海信标识.png" width="100%" height="100%" /></td>
                    </tr>
                </tbody>
            </table>

        </div>



    </div>



