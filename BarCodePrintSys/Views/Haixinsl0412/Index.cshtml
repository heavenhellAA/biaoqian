        function Check() {
            var cnbqywm = document.getElementById("cnbqywm").value;
            $.ajax({
                type: "GET",
                url: '../Datainself/Getdata',
                data: { scph: cnbqywm },
                dataType: "json",
                async: false,  //设置异步，必须先更改对应值才能做后续的传递参数操作
                success: function (data) {
                    //console.log(data.Tables[0].Rows[0]);
                    document.getElementById("khlh").value = data.Tables[0].Rows[0].customer_goods_id;
                    var curTime = new Date(data.Tables[0].Rows[0].datetime_source).format("yyyy/MM/dd"); //因为有xybself.js的方法
                    document.getElementById("scrq").value = curTime;
                    document.getElementById("wlms").value = data.Tables[0].Rows[0].customer_goods_name;
                    document.getElementById("sl").value = data.Tables[0].Rows[0].qty;
                    document.getElementById("scph").value = data.Tables[0].Rows[0].number;
                    document.getElementById("bz").value = data.Tables[0].Rows[0].goods_name + ' ' + data.Tables[0].Rows[0].size_id + ' ' + data.Tables[0].Rows[0].terminal;
                },
                error: function (res) {
                    console.log(res);
                    layer.msg('查询出错或者无数据！', { icon: 0 });
                }
            });
        }
        function showmsg() {
            var khlh = document.getElementById("khlh").value;//Code Number：客户料号
            var wlms = document.getElementById("wlms").value;//Description：物料描述
            var scrq = document.getElementById("scrq").value;//Produce date：生产日期
            var sl = document.getElementById("sl").value;//Quantity：数量
            var qc = document.getElementById("qc").value;//Check：检验员
            var scph = document.getElementById("scph").value;//Batch:生产批号
            var bz = document.getElementById("bz").value;//Remark：备注
            var slkbh = document.getElementById("slkbh").value;//Remark：试流卡编号
            var arr = scrq.split('/');
            var gysdm = document.getElementById("gysdm").value;

            $("#onecode-sl").barcode(sl+'-'+slkbh, 'code128', { barWidth: 1, barHeight: 32, fontSize: 6, showHRI: false });

            $("#khlh_msg").html(khlh);
            $("#wlms_msg").html(wlms);
            $("#scrq_msg").html(scrq);
            $("#sl_msg").html(sl);
            $("#onecode-sl_msg").html(sl + '-' + slkbh);
            $("#qc_msg").html(qc);
            $("#scph_msg").html(scph);
            $("#bz_msg").html(bz);
            $("#scy_msg").html(arr[1] + "月");
            $("#titile_msg").html("【"+gysdm +"】"+ " HuNan Aihua Group CO.,LTD");
        }

