                        bz: bz,
                        slkbh:slkbh,
                        num_print: num_print,
                        ylbd: ylbd,
                        cnbqywm: cnbqywm
                    },
                    dataType: "text",
                    success: function (data) {
                        var str = data.substring(7);
                        var scrq_32 = data.substring(2, 6);
                        var arr = str.split(',');
                        if (data.substring(0, 1) > 0) {
