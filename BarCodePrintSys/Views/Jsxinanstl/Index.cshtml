        function Check() {
            var cnbqywm = document.getElementById("cnbqywm").value;
            $.ajax({
                type: "GET",
                url: '../Datainself/Getdata',
                data: { scph: cnbqywm },
                dataType: "json",
                async: false,  //设置异步，必须先更改对应值才能做后续的传递参数操作
                success: function (data) {
                    //console.log(data.Tables[0].Rows[0]);
                    document.getElementById("sl").value = addPreZero(data.Tables[0].Rows[0].qty, 5);
                    //document.getElementById("scph").value = data.Tables[0].Rows[0].number;
                    //document.getElementById("ddh").value = data.Tables[0].Rows[0].customer_order_id;
                    document.getElementById("wlgg").value = data.Tables[0].Rows[0].goods_name + ' ' + data.Tables[0].Rows[0].size_id; //+ ' ' + data.Tables[0].Rows[0].terminal
                    document.getElementById("wllh").value = data.Tables[0].Rows[0].customer_goods_id;
                    document.getElementById("mpn").value = data.Tables[0].Rows[0].company_materials_id;
                    var curTime = new Date(data.Tables[0].Rows[0].datetime_source).format("yyyy/MM/dd"); //因为有xybself.js的方法
                    document.getElementById("scsj").value = curTime.replace('/', '').replace('/', '').substring(2, 8);
                    var date = new Date();
                    document.getElementById("dysj").value = date.format("yyyy/MM/dd").replace('/', '').replace('/', '');
                    document.getElementById("wlz").value = data.Tables[0].Rows[0].customer_goods_name;
                },
                error: function (res) {
                    console.log(res);
                    layer.msg('查询出错或者无数据！', { icon: 0 });
                }
            });
        }
