                    <option value="1">外箱</option>
                </select>
            </div>
            <div class="font" style="width: 400px;">
                组织代码:
				<select id="zzdm" style="width: 100px;">
                    <option value="408">408</option>
                    <option value="409">409</option>
                    <option value="439">439</option>
                </select>
            </div>
