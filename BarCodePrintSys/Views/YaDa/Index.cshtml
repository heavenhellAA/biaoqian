        function showmsg() {
            var zzdm = document.getElementById("zzdm").value;//组织代码
            var khlh = document.getElementById("khlh").value;//客户料号
            var pch = document.getElementById("pch").value;//批次号
            var sl = document.getElementById("sl").value;//数量
