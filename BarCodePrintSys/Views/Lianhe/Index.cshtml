        function Check() {
            var cnbqywm = document.getElementById("cnbqywm").value;
            $.ajax({
                type: "GET",
                url: '../Datainself/Getdata',
                data: { scph: cnbqywm },
                dataType: "json",
                async: false,  //设置异步，必须先更改对应值才能做后续的传递参数操作
                success: function (data) {
                    //console.log(data.Tables[0].Rows[0]);
                    document.getElementById("khlh").value = data.Tables[0].Rows[0].customer_goods_id;
                    document.getElementById("onecode2").value = data.Tables[0].Rows[0].customer_goods_id;
                    document.getElementById("onecode4").value = addPreZero(document.getElementById("gysdm").value,10);
                    Getrq(data.Tables[0].Rows[0].dc.substring(0,4));
                    document.getElementById("sl").value = data.Tables[0].Rows[0].qty;
                    document.getElementById("scph1").value = data.Tables[0].Rows[0].number;
                    document.getElementById("onecode6").value = addPreZero(data.Tables[0].Rows[0].number,19)
                    document.getElementById("onecode8").value = addPreZero(data.Tables[0].Rows[0].qty,5)
                    document.getElementById("cgddbh").value = data.Tables[0].Rows[0].customer_order_id;
                    document.getElementById("aslh").value = data.Tables[0].Rows[0].company_materials_id;
                    document.getElementById("aslhbcsm").value = data.Tables[0].Rows[0].company_materials_id;
                    document.getElementById("wlms").value = data.Tables[0].Rows[0].goods_name; //+ ' ' + data.Tables[0].Rows[0].size_id; + ' ' + data.Tables[0].Rows[0].terminal
                },
                error: function (res) {
                    console.log(res);
                    layer.msg('查询出错或者无数据！', { icon: 0 });
                }
            });
        }
        function Getrq(sczq) {
            $.ajax({
                type: "GET",
                url: '../Datainself/GetRqdata',
                data: { zqm: sczq },
                dataType: "json",
                async: false,  //设置异步，必须先更改对应值才能做后续的传递参数操作
                success: function (data) {
                    document.getElementById("scrq").value = data.Tables[0].Rows[0].s_scrq;
                    document.getElementById("yxq").value = data.Tables[0].Rows[0].s_yxq;
                },
                error: function (res) {
                    console.log(res);
                }
            });
        }
        ////流水号自动增长 
        function lsnum() {
            $.ajax({
                type: "GET",
                url: '../Lianhe/Getlsnum',
                dataType: "text",
                success: function (data) {
                    $("#bzbs2").val(data);
                },
                error: function (res) {
                    console.log(res);
                }
            });
        }
