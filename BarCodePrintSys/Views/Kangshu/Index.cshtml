            $("#zzzq_msg").html('<span>Marking D/C:</span>' + zzzq);
            $("#zq_msg").html('<span>D/C:</span>' + zzzq);
            $("#zzph_msg").html('<span>L/C:' + zzph);
            $("#clcd_msg").html('<span>C/C:</span>' + clcd);
            $("#jhcb_msg").html('<span>SITE:</span>' + jhcb);
            $("#ddh_msg").html('<span>P/O:</span>' + ddh);
