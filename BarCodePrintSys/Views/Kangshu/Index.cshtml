            var ycph = document.getElementById("ycph").value;//原厂批号
            var aslh = document.getElementById("aslh").value;//原厂料号
            var gysmc = document.getElementById("gysmc").value;//供应商名称

            var clbs = document.getElementById("clbs").value;
            if (clbs == "RoHS") {
                $("#clbs_msg").html('<span style="border: 1px solid; margin: 0 auto">' + clbs + '</span>');
            } else if (clbs == "HF") {
                $("#clbs_msg").html('<span style="border: 1px solid; margin: 0 auto">' + clbs + '</span>');
            }
            $("#khlh_msg").html('<span>P/N:</span>' + khlh);
