                        aslh: aslh,
                        gysmc: gysmc,
                        clbs:clbs,
                        num_print: num_print,
                        ylbd: ylbd
                    },
                    dataType: "text",
                    success: function (data) {
                        var str = data.substring(3);
                        var arr = str.split(',');
                        if (data.substring(0, 1) > 0) {
                            var num1 = $("#numcount").val();
                            var headstr = "<html><body>";
                            var footstr = "</body></html>";
                            var printData1 = '';
                            var fenye = '<div style="page-break-after:always;"></div>';    //分页


                            for (i = 0 ; i < num1 ; i++) {
                                lsh = arr[i];//流水号
                                $("#lsh_msg").html('<span>S/N:</span>' + lsh);
                                $('#datamatrix').html('');
                                $("#datamatrix").barcode(khlh + ';' + csdm + ';' + sl + ';' + zzzq + ';' + zzph + ';' + lsh + ';' + clcd + ';' + "NA;" + jhcb + ';' + ddh + ';' + zzzq + ';' + ycph + ';' + aslh + ';' + gysmc + ';' + "NA;", 'datamatrix', { barWidth: 1, barHeight: 20, fontSize: 6, showHRI: false, moduleSize: 2 });
