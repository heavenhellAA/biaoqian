        function Check() {
            var cnbqywm = document.getElementById("cnbqywm").value;
            $.ajax({
                type: "GET",
                url: '../Datainself/Getdata',
                data: { scph: cnbqywm },
                dataType: "json",
                async: false,  //设置异步，必须先更改对应值才能做后续的传递参数操作
                success: function (data) {
                    //console.log(data.Tables[0].Rows[0]);
                    document.getElementById("sl").value = data.Tables[0].Rows[0].qty;
                    //document.getElementById("wlxh").value = data.Tables[0].Rows[0].goods_name + ' ' + data.Tables[0].Rows[0].size_id; //+ ' ' + data.Tables[0].Rows[0].terminal
                    document.getElementById("ddh").value = data.Tables[0].Rows[0].customer_order_id;
                    if (document.getElementById("ddh").value.length <= 2) {
                        document.getElementById("ddh").value = "NA"
                    }
                    document.getElementById("zzph").value = data.Tables[0].Rows[0].number;
                    document.getElementById("khlh").value = data.Tables[0].Rows[0].customer_goods_id;
                    if (data.Tables[0].Rows[0].dc.substring(0, 1) == 'L') {
                        var date = new Date();
                        var str = date.format("yyyy-MM-dd");
                        var zqdm = str.substring(2, 3);
                        document.getElementById("zzzq").value = zqdm + data.Tables[0].Rows[0].dc.substring(1, 4);
                    } else {
                        document.getElementById("zzzq").value = data.Tables[0].Rows[0].dc;
                    }
                    //document.getElementById("zzzq").value = data.Tables[0].Rows[0].dc;
                    document.getElementById("aslh").value = data.Tables[0].Rows[0].company_materials_id;
                    getlsnum(data.Tables[0].Rows[0].dc)
                },
                error: function (res) {
                    console.log(res);
                    layer.msg('查询出错或者无数据！', { icon: 0 });
                }
            });
        }
        function getlsnum(zzph) {
            $.ajax({
                type: "POST",
                url: "/Kangshu/Getlsnum",
                data: {
                    zzph: zzph,
                },
                dataType: "text",
                success: function (data) {
                    document.getElementById("lsh").value = data;
                },
                error: function (res) {
                    console.log(res);
                    layer.msg('查询出错或者无数据！', { icon: 0 });
                }
            });
        }
        function showmsg() {
            var khlh = document.getElementById("khlh").value;//康舒料号
            var csdm = document.getElementById("csdm").value;//厂商代码
            var sl = document.getElementById("sl").value;//包装数量
            var zzzq = document.getElementById("zzzq").value;//制造周期
            var zzph = document.getElementById("zzph").value;//制造批号
