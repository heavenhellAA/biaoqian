<body>
    <div id="layerDemo" style="margin-bottom: 0;">
        <div class="layui-btn-container" style="display:none;">
            <button data-method="notice" class="layui-btn">弹出标签使用方法以及更新说明</button>
        </div>
    </div>
    <!--四川九州客户标签-->
    <div style="width: 100%; height: 430px; display: none" id="pint_wai">
        <div id="textarea" style="width: 400px; height: 360px; margin-top: 20px; line-height: 25px; margin-left: 50px; text-align: right; display: inline-block;">
            <div style="width: 400px;" class="font">
                厂内标签一维码:
				<input id="cnbqywm" type="text" onblur="Check();" />
            </div>
            <div class="font" style="width: 400px;">
                包装类型:
				<select id="packageType" disabled>
                    <option value="0">外箱</option>
                    <option value="1">最小包装</option>
                </select>
            </div>
            <div style="width: 400px;" class="font">
                打印日期:
				<input id="dyrq" type="text" readonly/>
            </div>
            <div style="width: 400px;" class="font">
                供应商代码:
				<input id="gysdm" type="text" value="1211"/>
            </div>
            <div style="width: 400px;" class="font">
                流水号:
				<input id="lsh" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                物料名称:
				<input id="wlmc" type="text" value="铝电解电容"/>
            </div>
            <div style="width: 400px;" class="font">
                客户料号:
				<input id="khlh" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                供应商:
				<select id="gys">
                    <option value="绵阳高新区资江电子元件有限公司">绵阳高新区资江电子元件有限公司</option>
                    <option value="益阳">益阳</option>
                </select>
            </div>
            <div style="width: 400px;" class="font">
                采购订单:
				<input id="cgdd" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                采购计划:
				<input id="cgjh" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                生产日期:
				<input id="scrq" type="text" minlength="6" maxlength="6"/>
            </div>
            <div style="width: 400px;" class="font">
                数量:
				<input id="sl" type="text" />
            </div>
            <div style="width: 400px;" class="font">
                打印张数:
				<input type="number" id="numcount" oninput="if(value > 999 || value < 1 ){alert('打印张数范围在1到999之间！');value = ''}" />
            </div>
            <div style="width: 400px;" class="font">
                是否为遗漏补打:
					<label>
                        <input type="checkbox" name="ylbd" value="1">是</label>
            </div>
            <div style="width: 400px; height: 45px; text-align: right;">
                @* <button id="Eliminate" class="bot" style="border: 5px solid #ffd68c; display: inline-block; background-color: orange; color: white;">清空</button>*@
                <button id="Printing" class="bot" style="border: 5px solid #ffd68c; display: inline-block; background-color: seagreen; color: white;">打印</button>
            </div>
        </div>




        <!--右侧打印区域-->
        <div id="PrintArea" style="width: 100mm; height: 50mm; vertical-align: top; display: inline-block; margin-left: 20px; white-space: nowrap; font-family: SimSun,monospace;">
            <!--使用表格显示-->
            <table style="border: thin; width: 98mm; height: 48mm; margin-left: 4px; margin-top: 4px;" rules="rows">
                <tbody>

                    <tr style="height: 18px">
                        <td colspan="2" id="lsm_msg" class="onecode-text" style="padding-left:10px;font:bolder 14px Arial;border-right-style:solid;"><span>(S)S/N:</span></td>
                    </tr>

                    <tr style="height:20px;">
                        <td colspan="2" id="onecode_lsm" class="onecode-text" style="border-right-style:solid;"></td>
                    </tr>

                    <tr style="height:15px;">
                        <td colspan="2" id="wlmc_msg" class="onecode-text" style="padding-left:10px;border-right-style:solid;"><span>(P)Name:</span></td>
                    </tr>

                    <tr style="height:18px;">
                        <td colspan="2" id="khlh_msg" class="onecode-text" style="padding-left:10px;font:bolder 14px Arial;border-right-style:solid;"><span>(P)P/N:</span></td>
                    </tr>

                    <tr style="height:20px;">
                        <td colspan="2" id="onecode_khlh" class="onecode-text" style="border-right-style:solid;"></td>
                    </tr>

                    <tr style="height:15px;">
                        <td colspan="2" id="gys_msg" class="onecode-text" style="padding-left:10px;border-right-style:solid;"><span>供应商:</span></td>
                    </tr>

                    <tr style="height:15px;">
                        <td colspan="2" id="cgdd_msg" class="onecode-text" style="padding-left:10px;border-right-style:solid;"><span>采购订单:</span></td>
                    </tr>

                    <tr style="height:15px;">
                        <td colspan="2" id="cgjh_msg" class="onecode-text" style="padding-left:10px;border-right-style:solid;"><span>采购计划:</span></td>
                    </tr>

                    <tr style="height:18px;">
                        <td colspan="1" id="sl_msg" class="onecode-text" style="width:180px;padding-left:10px;font:bolder 14px Arial;"><span>数量:</span></td>
                        <td colspan="1" rowspan="2" id="scrq2_msg" class="onecode-text" style="width:180px;font:bolder 14px Arial;border-right-style:solid;border-bottom-style:solid;"><span>生产日期:</span></td>
                    </tr>

                    <tr style="height:26px;">
                        <td colspan="1" id="onecode_sl" class="onecode-text" style="border-bottom-style:solid;"></td>
                    </tr>
                </tbody>
            </table>

        </div>

    </div>

